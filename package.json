{
  "name": "@udia/udia-client",
  "version": "0.2.0",
  "description": "Javascript Client for Udia",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom --coverage",
    "posttest": "yarn lint",
    "eject": "react-scripts-ts eject",
    "lint": "tslint -c tslint.json --project .",
    "deploy": "yarn run build && aws s3 sync build/ s3://udia-amazon --delete",
    "prepublishOnly": "yarn test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/udia-software/udia-client.git"
  },
  "homepage": "https://udia.ca",
  "author": {
    "name": "Alexander Wong",
    "email": "alex@udia.ca",
    "url": "https://udia.ca"
  },
  "license": "AGPL-3.0",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "^0.1.0-11",
    "@types/luxon": "^1.2.0",
    "apollo-cache-inmemory": "^1.2.2",
    "apollo-client": "^2.3.2",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "^1.0.8",
    "apollo-utilities": "^1.0.15",
    "buffer": "^5.1.0",
    "graphql": "^0.13.0",
    "graphql-tag": "^2.9.2",
    "localforage": "^1.7.1",
    "luxon": "^1.3.0",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "styled-components": "^3.3.2",
    "subscriptions-transport-ws": "^0.9.9"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "~8.10.19",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-test-renderer": "^16.0.1",
    "@types/ws": "^5.1.2",
    "react-test-renderer": "^16.4.1",
    "typescript": "^2.9.1"
  }
}
